{
  "name": "vue-trame_color_opacity_editor",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/trame_color_opacity_editor.umd.js",
  "unpkg": "./dist/trame_color_opacity_editor.umd.js",
  "module": "./dist/trame_color_opacity_editor.mjs",
  "exports": {
    ".": {
      "import": "./dist/trame_color_opacity_editor.mjs",
      "require": "./dist/trame_color_opacity_editor.umd.js"
    }
  },
  "scripts": {
    "dev": "vite build --emptyOutDir --watch ",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build --emptyOutDir",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@colormap/core": "^0.2.0"
  },
  "peerDependencies": {
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.5.1",
    "jiti": "^2.6.1",
    "semantic-release": "^25.0.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.1.5"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ]
}
